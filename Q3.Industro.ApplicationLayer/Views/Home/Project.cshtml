@model Q3.Industro.ModelLayer.Models.Projects.ProjectCollection

@{
    ViewBag.Title = "Project";
    Layout = "~/Views/Shared/_Layout3.cshtml";
}

<!-- Navbar Start -->
<nav class="navbar navbar-expand-lg bg-white navbar-light sticky-top py-0 pe-5">
    <a href="@Url.Action("Index","Home")" class="navbar-brand ps-5 me-0">
        <h1 class="text-white m-0">Industro</h1>
    </a>
    <button type="button" class="navbar-toggler me-0" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarCollapse">
        <div class="navbar-nav ms-auto p-4 p-lg-0">
            <a href="@Url.Action("Index","Home")" class="nav-item nav-link">Home</a>
            <a href="@Url.Action("About","Home")" class="nav-item nav-link">About</a>
            <a href="@Url.Action("Service","Home")" class="nav-item nav-link">Services</a>
            <div class="nav-item dropdown">
                <a href="#" class="nav-link dropdown-toggle active" data-bs-toggle="dropdown">Pages</a>
                <div class="dropdown-menu bg-light m-0">
                    <a href="@Url.Action("Project","Home")" class="dropdown-item">Projects</a>
                    <a href="@Url.Action("Feature","Home")" class="dropdown-item">Features</a>
                    <a href="@Url.Action("OurTeam","Home")" class="dropdown-item">Our Team</a>
                    <a href="@Url.Action("Testimonial","Home")" class="dropdown-item">Testimonial</a>
                    <a href="@Url.Action("Error404","Home")" class="dropdown-item">404 Page</a>
                </div>
            </div>
            <a href="@Url.Action("Contact","Home")" class="nav-item nav-link">Contact</a>
        </div>
        <a href="" class="btn btn-primary px-3 d-none d-lg-block">Get A Quote</a>
    </div>
</nav>
<!-- Navbar End -->
<!-- Page Header Start -->
<div class="container-fluid page-header py-5 mb-5 wow fadeIn" data-wow-delay="0.1s">
    <div class="container py-5">
        <h1 class="display-3 text-white animated slideInRight">Projects</h1>
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb animated slideInRight mb-0">
                <li class="breadcrumb-item"><a href="#">Home</a></li>
                <li class="breadcrumb-item"><a href="#">Pages</a></li>
                <li class="breadcrumb-item active" aria-current="page">Projects</li>
            </ol>
        </nav>
    </div>
</div>
<!-- Page Header End -->
<!-- Project Start -->
<div class="container-fluid py-5 my-5 px-0">
    <div class="text-center mx-auto wow fadeIn" data-wow-delay="0.1s" style="max-width: 600px;">
        <p class="fw-medium text-uppercase text-primary mb-2">Our Projects</p>
        <h1 class="display-5 mb-5">See What We Have Completed Recently</h1>
    </div>
    <div class="owl-carousel project-carousel bg-dark wow fadeIn" data-wow-delay="0.1s">
        @Html.Partial("_Project", Model.Projects)
    </div>
</div>
<!-- Project End -->
